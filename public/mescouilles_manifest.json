{
  "manifest_version": 3,
  "name": "Eden for Youtube",
  "description": "pre aperçu du potentiel de Eden pour libérer votre potentiel",
  "version": "1.0",
  "icons": {
    "16": "./icons/logo16.png",
    "48": "./icons/logo48.png",
    "128": "./icons/logo128.png"
  },
  "action":{
    "default_popup": "index.html",
    "default_title": "Eden for Youtube"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions":[
    "tabs", 
    "activeTab", 
    "storage",
    "scripting",
    "cookies",
    "identity",
    "windows",
    "debugger"
  ],
  "host_permissions":[
    "https://youtube.com/*"
  ]
}